  data-init="{{sliderBox.initHeight}}"
  data-min="{{sliderBox.minHeight}}"
  bindtouchstart="{{test.touchstart}}"
  bindtouchmove="{{test.touchmove}}"
  bindtouchend="{{test.touchend}}"
  bindtouchcancel="{{test.touchend}}"

>
  <view class="mock_box"></view>
</view>
