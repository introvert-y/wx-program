<wxs src="./index.wxs" module="test"></wxs>


<!-- 确定了，以星盘首页的形式来处理冒出一部分的情况 -->
<view class="bg"></view>
<view
  class="slider_box"
  style="transform: translate3d(0, {{sliderBox.height - sliderBox.initHeight}}px, 0)"
