var funcA = function (e, ins) {
  // console.log('source is', JSON.stringify(e))
  var scrollTop = e.detail.scrollTop;
  console.log('scrollTop,', scrollTop);
  if (scrollTop > 100) {
    ins.selectComponent('.page-group').addClass('page-group-position')
  } else {
    ins.selectComponent('.page-group').removeClass('page-group-position')

  }
}
module.exports = {
  funcA: funcA
}
